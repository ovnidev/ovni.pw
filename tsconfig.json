{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {

      "@/*": [ "src/*" ],

      "@component/*": [ "src/components/*" ],
      "@layouts/*": [ "src/layouts/*" ],
      "@pages/*": [ "src/pages/*" ],
      "@interfaces/*": [ "src/interfaces/*" ],
      "@styles/*": [ "src/assets/styles/*" ],
      "@logic/*": [ "src/logic/*" ]

    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}